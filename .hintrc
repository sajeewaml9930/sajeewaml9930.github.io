{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/parsing": "off",
    "disown-opener": "off",
    "compat-api/css": "off",
    "axe/structure": [
      "default",
      {
        "listitem": "off"
      }
    ],
    "axe/name-role-value": "off",
    "no-inline-styles": "off",
    "meta-charset-utf-8": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "img[loading]"
        ]
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off"
      }
    ]
  }
}